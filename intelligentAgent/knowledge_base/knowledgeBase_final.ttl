focudata:Hittite a focu:Course ;
    focu:course_description """
Prerequisite: LING 336 or equivalent. The fundamentals of Hittite grammar are presented through the extensive reading of texts, both in transliteration and cuneiform. Considerable attention is given to problems of comparative grammar.""" ;
    focu:course_name "Hittite" ;
    focu:course_number "461" ;
    focu:course_subject "LING" ;
    rdfs:seeAlso "http://www.concordia.ca/academics/undergraduate/calendar/current/sec31/31-060.html#arabic" .

focudata:Homeric_Greek a focu:Course ;
