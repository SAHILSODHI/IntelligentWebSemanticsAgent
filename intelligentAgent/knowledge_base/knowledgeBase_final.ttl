    rdfs:seeAlso "http://www.concordia.ca/academics/graduate/calendar/current/fofa.html" .

focudata:Focus_on_Theory a focu:Course ;
    focu:course_description "Description not available" ;
    focu:course_name "Focus on Theory" ;
    focu:course_number "A:" ;
